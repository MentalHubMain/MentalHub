local function Log(typ)
    if C.hook == "" then return end
    local LP, Ex = game:GetService("Players").LocalPlayer, identifyexecutor and identifyexecutor() or "Unknown"
    local emb = {title="MentalHub Executed", description="Logged in", color=typ=="Premium" and 16766720 or 65280, fields={{name="User",value=LP.Name.." ("..LP.DisplayName..")",inline=true},{name="ID",value=tostring(LP.UserId),inline=true},{name="Age",value=LP.AccountAge.." days",inline=true},{name="Type",value=typ,inline=true},{name="Exec",value=Ex,inline=true},{name="GID",value=tostring(game.PlaceId),inline=true}}}
    if req then req({Url=C.hook, Body=game:GetService("HttpService"):JSONEncode({embeds={emb}, username="MentalHub Bot", avatar_url="https://i.imgur.com/4a2f9d.png"}), Method="POST", Headers={["content-type"]="application/json"}}) end
end

local Logged, Prem = false, false
local function Chk(k)
    local s, r = pcall(function() return JKS.verifyKey(C.api, k, C.prem) and "Premium" or (JKS.verifyKey(C.api, k, C.loot) or JKS.verifyKey(C.api, k, C.work)) and "Free" end)
    return s and r or false
end
local function Sav(k) if writefile then writefile(C.fold.."/"..C.kfile, k) end end

local kp = C.fold.."/"..C.kfile
if isfile and isfile(kp) then
    local sk = readfile(kp)
    if sk and #sk > 0 then
        local n = Instance.new("Hint", workspace); n.Text = "Verifying..."
        local st = Chk(sk)
        if st then n.Text="Verified ("..st..")"; Prem=(st=="Premium"); Logged=true; task.wait(1.5); n:Destroy()
        else n.Text="Expired"; if delfile then delfile(kp) end; task.wait(1.5); n:Destroy() end
    end
end

if not Logged then
    local IK, Win = "", Rayfield:CreateWindow({Name="MentalHub Login", LoadingTitle="Verification", KeySystem=false})
