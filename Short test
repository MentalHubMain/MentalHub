local TSL = RS:FindFirstChild("TS") and RS.TS:FindFirstChild("lists")
local Dbg = true
local function DPrint(m) if Dbg and (m:find("Error") or m:find("Failed")) then warn("Log: "..m) end end

local CV = {WS=16, WSE=false, IJ=false, Fly=false, FSpd=1, Clip=false, AFK=true, Staff=true}
local BG, BV
local function Fly()
    local c = LP.Character; if not c or not c:FindFirstChild("HumanoidRootPart") then return end
    local r, h = c.HumanoidRootPart, c:FindFirstChild("Humanoid")
    if BG then BG:Destroy() end; if BV then BV:Destroy() end
    BG = Instance.new("BodyGyro", r); BG.P=9e4; BG.maxTorque=Vector3.new(9e9,9e9,9e9); BG.cframe=r.CFrame
    BV = Instance.new("BodyVelocity", r); BV.velocity=Vector3.zero; BV.maxForce=Vector3.new(9e9,9e9,9e9)
    repeat task.wait()
        if not CV.Fly or not h then break end
        h.PlatformStand = true
        local cam, md = workspace.CurrentCamera, h.MoveDirection
        local s = CV.FSpd * 50
        if md.Magnitude > 0 then
            local lv, rv = cam.CFrame.LookVector, cam.CFrame.RightVector
            BV.velocity = (lv * md:Dot(lv * Vector3.new(1,0,1)) * s) + (rv * md:Dot(rv * Vector3.new(1,0,1)) * s)
        else BV.velocity = Vector3.zero end
        BG.cframe = cam.CFrame
    until not CV.Fly
    if h then h.PlatformStand = false end; if BG then BG:Destroy() end; if BV then BV:Destroy() end
end

game:GetService("UserInputService").JumpRequest:Connect(function() if CV.IJ and LP.Character then LP.Character:FindFirstChildOfClass('Humanoid'):ChangeState("Jumping") end end)
local NCL; local function TogClip(v) if v then NCL=game:GetService("RunService").Stepped:Connect(function() if LP.Character then for _,p in pairs(LP.Character:GetChildren()) do if p:IsA("BasePart") then p.CanCollide=false end end end end) else if NCL then NCL:Disconnect() end end end
