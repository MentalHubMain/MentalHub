local MShop = {Egg={"Starter","Novice","Forest","Polar","Tropical","Exotic"}, Event={"Atlantis Egg","Atlantis Crate","Trident Throne","Poseidon Statue","Kings Throne","Atlantis Lightpole","Atlantis Banner"}, Merch={"Zoo","Wild","Punk","Boba","Ashen"}, Gear={}, Bait={}}
local function AddAI(cat, id) if not table.find(MShop[cat], id) then table.insert(MShop[cat], id) end end
if TSL then
    local gf = TSL:FindFirstChild("game")
    if gf then
        local bf = gf:FindFirstChild("bait-packs.list") or gf:FindFirstChild("bait-packs") or gf:FindFirstChild("bait")
        if bf then
            local s, d = pcall(require, bf)
            if s and d then for id, i in pairs(d.Baits or d.BaitPacks) do if i.source == (d.BaitSource or {}).BaitShop or i.source == "BaitShop" then AddAI("Bait", id) end end end
        end
    end
    for _, m in ipairs(TSL:GetDescendants()) do
        if m:IsA("ModuleScript") then
            local s, d = pcall(require, m)
            if s and d.Gears then for id, i in pairs(d.Gears) do if i.source == (d.GearSource or {}).GearShop or i.source == "GearShop" then AddAI("Gear", id) end end end
        end
    end
end
table.sort(MShop.Gear); table.sort(MShop.Bait); if #MShop.Gear==0 then MShop.Gear={"BasicAutoFeeder","FoodScoop"} end; if #MShop.Bait==0 then MShop.Bait={"Starter","Novice"} end
local function Filter(l, q) if not q or q=="" then return l end local f={}; q=q:lower(); for _,i in ipairs(l) do if i:lower():find(q) then table.insert(f,i) end end return f end

local L, Sels, Sets, Togs = {Egg=false,Gear=false,Bait=false,Evt=false,Mer=false,Col=false,Sac=false,Crt=false,Sell=false,EvS=false,HL=false}, {Egg={},Gear={},Bait={},Evt={},Mer={}}, {SD=300,ESD=300,CD=10}, {}
local Q, PIDs = {}, {}
local function isUUID(s) return type(s)=="string" and s:match("%x%x%x%x%x%x%x%x%-%x%x%x%x%-%x%x%x%x%-%x%x%x%x%-%x%x%x%x%x%x%x%x%x%x%x%x") end
local function rFind(d) if type(d)=="table" then for k,v in pairs(d) do if isUUID(k) and not PIDs[k] then table.insert(Q,k); PIDs[k]=true end if isUUID(v) and not PIDs[v] then table.insert(Q,v); PIDs[v]=true end if type(v)=="table" then rFind(v) end end elseif isUUID(d) and not PIDs[d] then table.insert(Q,d); PIDs[d]=true end end

getgenv().GCon = getgenv().GCon or {}; for _,c in ipairs(getgenv().GCon) do c:Disconnect() end; getgenv().GCon = {}
for _,r in pairs(Rems:GetDescendants()) do if r:IsA("RemoteEvent") and (r.Name:find("state") or r.Name:find("replecs") or r.Name:find("sync") or r.Name:find("update")) and not (r.Name:find("fish") or r.Name:find("weather") or r.Name:find("mutation")) then table.insert(getgenv().GCon, r.OnClientEvent:Connect(function(...) rFind({...}) end)) end end
getgenv().GLoop = getgenv().GLoop or {}; for _,t in ipairs(getgenv().GLoop) do task.cancel(t) end; getgenv().GLoop = {}
local function AddL(f) table.insert(getgenv().GLoop, task.spawn(f)) end
local function Buy(r, i) pcall(function() Rems[r]:FireServer(i) end) end

local function TEgg(v) L.Egg=v; if v then AddL(function() while L.Egg do for _,i in ipairs(Sels.Egg) do if not L.Egg then break end Buy("shop.purchaseEgg",i) end task.wait(0.2) end end) end end
local function THL(v) L.HL=v; if v then AddL(function() while L.HL do if #Q>0 then local id=table.remove(Q,1); if not Rems:FindFirstChild("pets.hatchEgg") then DPrint("Error 404") end task.spawn(function() local s,e=pcall(function() Rems["pets.hatchEgg"]:InvokeServer(id) end) if not s then DPrint("Err: "..tostring(e)) end end) task.spawn(function() pcall(function() Rems["pets.hatchEgg"]:FireServer(id) end) end) task.wait(0.05) else task.wait(0.5) end end end) end end
local function TGear(v) L.Gear=v; if v then AddL(function() while L.Gear do for _,i in ipairs(Sels.Gear) do if not L.Gear then break end Buy("shop.purchaseGear",i) end task.wait(0.1) end end) end end
local function TBait(v) L.Bait=v; if v then AddL(function() while L.Bait do for _,i in ipairs(Sels.Bait) do if not L.Bait then break end Buy("shop.purchaseBait",i) end task.wait(0.1) end end) end end
local function TEvt(v) L.Evt=v; if v then AddL(function() while L.Evt do for _,i in ipairs(Sels.Evt) do if not L.Evt then break end if i=="Atlantis Egg" then pcall(function() Rems["shop.purchaseEventItem"]:FireServer("egg:Atlantis") end) elseif i=="Atlantis Crate" then Buy("shop.purchaseEventItem","baitpack:Atlantis") elseif i=="Trident Throne" then Buy("shop.purchaseCosmetic","TridentThrone") elseif i=="Poseidon Statue" then Buy("shop.purchaseCosmeticExtra","PoseidonStatue") elseif i=="Kings Throne" then Buy("shop.purchaseCosmetic","KingsThrone") elseif i=="Atlantis Lightpole" then Buy("shop.purchaseCosmeticExtra","AtlantisLightpole") elseif i=="Atlantis Banner" then Buy("shop.purchaseCosmeticExtra","AtlantisBanner") else Buy("shop.purchaseEventItem",i) end end task.wait(0.2) end end) end end
local function TMer(v) L.Mer=v; if v then AddL(function() while L.Mer do for _,i in ipairs(Sels.Mer) do if not L.Mer then break end pcall(function() Rems["merchant.purchaseItem"]:FireServer("travelling",i) end) end task.wait(0.5) end end) end end
local function TSac(v) L.Sac=v; if v then AddL(function() while L.Sac do pcall(function() Rems["sellFish.sacrificeAllFish"]:FireServer() end) task.wait(1) end end) end end
local function TES(v) L.EvS=v; if v then AddL(function() while L.EvS do pcall(function() Rems["sellFish.sellAllEventFish"]:FireServer() end) task.wait(Sets.ESD) end end) end end
local function TCrt(v) L.Crt=v; if v then AddL(function() while L.Crt do pcall(function() Rems["store.openBaitPack"]:FireServer("Atlantis:normal") end) task.wait(0.05) end end) end end
local function TCol(v) L.Col=v; if v then AddL(function() while L.Col do pcall(function() Rems["crafting.collectCraft"]:FireServer("gear") end) task.wait(Sets.CD<1 and 10 or Sets.CD) end end) end end
local function TSell(v) L.Sell=v; if v then AddL(function() while L.Sell do pcall(function() Rems["sellFish.sellAllFish"]:FireServer() end) task.wait(Sets.SD) end end) end end

