local CfTab, CName, CFold, CExt, ALoad = Win:CreateTab("Config"), "", "GardenAdmin", ".json", "autoload.txt"
local function GetCs() local l,n=listfiles(CFold),{} for _,f in ipairs(l) do if not f:find(ALoad) then table.insert(n, f:match("([^\\/]+)"..CExt.."$")) end end return n end
local function LoadC(n)
    local p = CFold.."/"..n..CExt; if isfile(p) then
        local s,d=pcall(function() return HS:JSONDecode(readfile(p)) end)
        if s then
            if d.Sels then Sels=d.Sels; if EDD then EDD:Set(Sels.Egg) end; if GDD then GDD:Set(Sels.Gear) end; if BDD then BDD:Set(Sels.Bait) end; if EvDD then EvDD:Set(Sels.Evt) end; if MDD then MDD:Set(Sels.Mer) end end
            if d.Sets then for k,v in pairs(d.Sets) do Sets[k]=v end; if not Sets.CD then Sets.CD=10 end end
            if d.L then for k,v in pairs(d.L) do if Togs[k] and Togs[k].Set then Togs[k]:Set(true) end end end
            return true
        end
    end return false
end
local CDD = CfTab:CreateDropdown({Name="List",Options=GetCs(),CurrentOption={},MultipleOptions=false,Callback=function(v) CName=(type(v)=="table" and v[1] or v) end})
CfTab:CreateInput({Name="Name",PlaceholderText="Name...",RemoveTextAfterFocusLost=false,Callback=function(t) CName=t end})
CfTab:CreateButton({Name="Save",Callback=function() if CName=="" then return end writefile(CFold.."/"..CName..CExt, HS:JSONEncode({Sels=Sels,Sets=Sets,L=L})) CDD:Refresh(GetCs(),true) Rayfield:Notify({Title="Saved",Content="Done",Duration=2}) end})
CfTab:CreateButton({Name="Load",Callback=function() if CName~="" and LoadC(CName) then Rayfield:Notify({Title="Loaded",Content="Done",Duration=2}) end end})
CfTab:CreateButton({Name="Delete",Callback=function() if CName~="" and isfile(CFold.."/"..CName..CExt) then delfile(CFold.."/"..CName..CExt) CDD:Refresh(GetCs(),true) Rayfield:Notify({Title="Deleted",Content="Done",Duration=2}) end end})
CfTab:CreateButton({Name="Refresh",Callback=function() CDD:Refresh(GetCs(),true) end})
CfTab:CreateSection("Autoload"); CfTab:CreateButton({Name="Set Autoload",Callback=function() if CName~="" then writefile(CFold.."/"..ALoad,CName) Rayfield:Notify({Title="Set",Content=CName,Duration=2}) end end})
CfTab:CreateButton({Name="Reset Autoload",Callback=function() if isfile(CFold.."/"..ALoad) then delfile(CFold.."/"..ALoad) end end})
CfTab:CreateSection("Sys"); CfTab:CreateButton({Name="Unload",Callback=function() Rayfield:Destroy(); for _,t in ipairs(getgenv().GLoop) do task.cancel(t) end; getgenv().GLoop={}; for _,c in ipairs(getgenv().GCon) do c:Disconnect() end; getgenv().GCon={} end})

if isfile(CFold.."/"..ALoad) then local an=readfile(CFold.."/"..ALoad) if LoadC(an) then Rayfield:Notify({Title="Autoload",Content=an,Duration=3}) end end
CfTab:CreateSection("Info"); CfTab:CreateButton({Name="Discord",Callback=function() setclipboard(C.disc); Rayfield:Notify({Title="Copied",Content="Link!",Duration=2}) end})
Rayfield:LoadConfiguration()
