    Tab:CreateButton({Name="Verify", Callback=function() if IK=="" then return end local st=Chk(IK) if st then Prem=(st=="Premium"); Sav(IK); Logged=true; Rayfield:Destroy() else Rayfield:Notify({Title="Error", Content="Invalid Key", Duration=3}) end end})
    Tab:CreateButton({Name="Get Key (LootLabs)", Callback=function() local l=JKS.getLink(C.api, C.prov_loot, C.loot) if l and not l:find("Error") then setclipboard(l) Rayfield:Notify({Title="LootLabs", Content="Copied!", Duration=3}) end end})
    Tab:CreateButton({Name="Get Key (WorkInk)", Callback=function() local l=JKS.getLink(C.api, C.prov_work, C.work) if l and not l:find("Error") then setclipboard(l) Rayfield:Notify({Title="WorkInk", Content="Copied!", Duration=3}) end end})
    Tab:CreateButton({Name="Buy Premium", Callback=function() setclipboard(C.disc); Rayfield:Notify({Title="Discord", Content="Joined!", Duration=3}) end})
end
